{
"version:2,
  "build":[
    {
      "src": "src/index.js",
      "use":"@vercel/node"
    }
  ],
  "routes":[
    {
      "src":"/(.*)",
      "dest": "src/index.js",
      "method":["GET","POST","PUT","DELETE","PATCH","OPTIONS"],
      "headers":{
        "Access-Control-Allow-Origin":"*"
        }
    }
  ]
}

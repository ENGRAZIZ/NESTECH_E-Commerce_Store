{
"version":2,
  "build":[
    {
      "src":"index.js",
      "use":"@vercel/node"
    }
  ].
  "routes":[
    {
      "src":"/(.*)",
      "dest":"index.js",
      "method":["GET","POST","PUT","DELETE","PATCH","OPTIONS"],
      "headers":{
        "Access-Control-Allow-Origin":"*"
      }
    }
  ]
}

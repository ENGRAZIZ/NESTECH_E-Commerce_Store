{
{
  "functions": {
    "api/*.js": {
      "memory": 512,
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api",
      "header":{'Access-Control-Allow-Origin', '*'}
    }
  ]
}

}
